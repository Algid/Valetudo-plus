{
  "/api/v2/robot/capabilities/MapSegmentCleanOrderCapability": {
    "put": {
      "tags": [
        "MapSegmentCleanOrderCapability"
      ],
      "summary": "Order map segments",
      "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "action": {
                  "type": "string",
                  "enum": [
                    "order_segments"
                  ]
                },
                "segment_ids": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "description": "Segment IDs in order",
                    "minimum": 1
                  }
                }
              }
            }
          }
        }
      },
      "responses": {
        "200": {
          "$ref": "#/components/responses/200"
        },
        "400": {
          "$ref": "#/components/responses/400"
        }
      }
    }
  },
  "/api/v2/robot/capabilities/MapSegmentCleanOrderCapability/properties": {
    "get": {
      "tags": [
        "MapSegmentCleanOrderCapability"
      ],
      "summary": "Get various capability-related properties",
      "responses": {
        "200": {
          "description": "Ok",
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        }
      }
    }
  }
}
